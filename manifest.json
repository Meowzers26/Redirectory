{
    "manifest_version": 3,
    "name": "Redirectory",
    "description": "Redirect YouTube links",
    "version": "1.0",
    "background": {
        "service_worker": "background.js"
    },
    "permissions": [
        "webRequest",
        "webRequestBlocking",
        "https://www.youtube.com/*"
    ]
}