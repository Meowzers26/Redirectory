{
    "manifest_version": 3,
    "name": "Redirectory",
    "description": "Redirect YouTube links",
    "version": "1.0",
    "declarative_net_request": {
        "rule_resources" :[{
            "id": "ruleset_1",
            "enabled": true,
            "path": "rules.json"
        }]
    },
    "permissions": [
        "declarativeNetRequestWithHostAccess",
        "declarativeNetRequestFeedback"
    ],
    "host_permissions": [
        "https://www.youtube.com/*"
    ]
}