{
    "manifest_version": 3,
    "name": "Redirectory",
    "description": "Redirect YouTube links",
    "version": "1.0",
    "icons": "images/icon-redirect.png",
    "background": {
        "scripts": ["background.js"]
    },
    "permissions": [
        "webRequest",
        "webRequestBlocking",
        "https://www.youtube.com/*"
    ]
}